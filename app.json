{
  "expo": {
    "name": "bolt-expo-nativewind",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "package": "com.yourcompany.boltexponativewind",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_GOOGLE_MAPS_API_KEY_HERE"
        }
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.boltexponativewind",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location for time tracking purposes.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to your location for time tracking purposes.",
        "NSLocationAlwaysUsageDescription": "This app needs access to your location for time tracking purposes."
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location for time tracking.",
          "locationAlwaysPermission": "Allow $(PRODUCT_NAME) to use your location for time tracking.",
          "locationWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location for time tracking."
        }
      ],
      [
        "expo-maps",
        {
          "androidApiKey": "YOUR_GOOGLE_MAPS_API_KEY_HERE",
          "iosApiKey": "YOUR_GOOGLE_MAPS_IOS_API_KEY_HERE"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d4dcbf61-8298-41cb-9e73-4183c0a85420"
      }
    }
  }
}
